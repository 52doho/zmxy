{
  "name": "zmxy",
  "version": "2.0.3",
  "description": "Zhima credit unofficial SDK",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/bmqb/zmxy.git"
  },
  "scripts": {
    "pretest": "npm run build",
    "prestart": "npm run build",
    "prepublish": "rm -rf ./lib && npm run build",
    "ava": "LOG_LEVEL=error NODE_ENV=test node ./node_modules/.bin/ava --verbose --serial",
    "test": "nyc -a --reporter=lcov --reporter=text --reporter=html npm run ava",
    "lint": "eslint src/* --ext .js",
    "build": "babel -d lib/ src/"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "zmxy",
    "zhima credit",
    "zmxy SDK",
    "zhimaxinyong SDK",
    "芝麻信用",
    "芝麻信用SDK"
  ],
  "main": "index.js",
  "author": "AlloVince",
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/bootstrap.js",
      "!test/_*/**/*.js",
      "!test/swagger/_*/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": false,
    "tap": false,
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [
      "babel-core/register"
    ],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/config/index.js"
    ]
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015-node4": "^2.1.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "nock": "^9.0.13",
    "nyc": "^11.0.2",
    "pre-commit": "^1.2.2"
  }
}
